{
  "name": "ember-raf-scheduler",
  "version": "0.0.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:html-next/ember-raf-scheduler.git"
  },
  "license": "MIT",
  "author": "",
  "exports": {
    ".": "./src/index.js",
    "./*": "./src/*.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "src"
  ],
  "scripts": {
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm run format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepack": "rollup --config",
    "start": "vite dev",
    "test": "NODE_ENV=development vite build --mode=development && testem --file testem.cjs ci"
  },
  "dependencies": {
    "@ember/test-waiters": "^2.0.0 || ^3.0.0 || ^4.0.0",
    "@embroider/addon-shim": "^1.8.9",
    "@embroider/macros": "^1.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/runtime": "^7.25.6",
    "@ember/test-helpers": "^5.2.1",
    "@ember/test-waiters": "^4.1.0",
    "@embroider/core": "^4.0.3",
    "@embroider/vite": "^1.1.3",
    "@eslint/js": "^9.17.0",
    "@glimmer/component": "^2.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "babel-plugin-ember-template-compilation": "^2.2.5",
    "concurrently": "^9.0.1",
    "ember-qunit": "^9.0.2",
    "ember-resolver": "^13.1.0",
    "ember-source": "^6.3.0",
    "ember-template-lint": "^7.7.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-ember": "^12.3.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "globals": "^16.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-ember-template-tag": "^2.0.4",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "release-plan": "^0.16.0",
    "testem": "^3.16.0",
    "vite": "^6.2.4"
  },
  "packageManager": "pnpm@10.11.0",
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs"
  }
}
